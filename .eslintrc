{
  "extends": "airbnb",
  "rules": {
    "object-shorthand": ["error", "always", { "avoidQuotes": false }],
    "object-curly-newline": ["error", {
      "ObjectExpression": { "minProperties": 6, "multiline": true, "consistent": true },
      "ObjectPattern": { "minProperties": 6, "multiline": true, "consistent": true }
    }]
  }
}